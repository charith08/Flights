<p id="notice"><%= notice %></p>

<h1>Economies</h1>

<table>
  <thead>
    <tr>
      <th>Row</th>
      <th>Seats in row</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @economies.each do |economy| %>
      <tr>
        <td><%= economy.row %></td>
        <td><%= economy.seats_in_row %></td>
        <td><%= link_to 'Show', economy %></td>
        <td><%= link_to 'Edit', edit_economy_path(economy) %></td>
        <td><%= link_to 'Destroy', economy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Economy', new_economy_path %>
