<h1>Seats</h1>

<table>
  <thead>
    <tr>
      <th>Class</th>
      <th>Spnr</th>
      <th>Row</th>
      <th>Seats in row</th>
      <th>Class pnr</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seats.each do |seat| %>
      <tr>
        <td><%= seat.class_id %></td>
        <td><%= seat.spnr %></td>
        <td><%= seat.row %></td>
        <td><%= seat.seats_in_row %></td>
        <td><%= seat.class_pnr %></td>
        <td><%= link_to 'Show', seat %></td>
        <td><%= link_to 'Edit', edit_seat_path(seat) %></td>
        <td><%= link_to 'Destroy', seat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
